{
  "name": "money-plant-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:dev": "ng serve --proxy-config proxy.conf.json --port 4200",
    "build": "npm run build:dashboards && npm run build:criteria-builder && ng build",
    "build:dashboards": "ng build dashboards",
    "build:criteria-builder": "ng build criteria-builder",
    "build:criteria-builder:dev": "ng build criteria-builder --configuration development",
    "build:prod": "npm run build:dashboards && npm run build:criteria-builder && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:criteria-builder": "ng test criteria-builder",
    "test:criteria-builder:watch": "ng test criteria-builder --watch",
    "lint:criteria-builder": "ng lint criteria-builder",
    "pack:criteria-builder": "cd projects/criteria-builder && npm run pack"
  },
  "private": true,
  "dependencies": {
    "@analogjs/vite-plugin-angular": "^1.21.2",
    "@angular/animations": "^20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "^20.3.3",
    "@angular/compiler": "^20.3.3",
    "@angular/core": "^20.3.3",
    "@angular/forms": "^20.3.3",
    "@angular/platform-browser": "^20.3.3",
    "@angular/platform-browser-dynamic": "^20.3.3",
    "@angular/router": "^20.3.3",
    "@azure/msal-angular": "^4.0.16",
    "@azure/msal-browser": "^4.18.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@primeng/themes": "^20.0.0",
    "@stomp/stompjs": "^7.1.1",
    "angular-gridster2": "^20.0.0",
    "chart.js": "^4.4.9",
    "echarts": "^5.6.0",
    "echarts-map-collection": "^1.0.2",
    "googleapis": "^155.0.0",
    "ngx-echarts": "^20.0.0",
    "ngx-print": "^3.0.0",
    "odata-query": "^8.0.3",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.2.0",
    "rxjs": "~7.8.0",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.4",
    "@angular/build": "^20.3.4",
    "@angular/cli": "^20.3.4",
    "@angular/compiler-cli": "^20.3.3",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^22.15.29",
    "@types/sockjs-client": "^1.5.4",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "typescript": "~5.8.3"
  }
}